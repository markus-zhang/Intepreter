# Starting the program
print('Program started')

# Variable initialization
counter = 0
limit = 10
inner_limit = 3
outer_flag = 'start'
inner_flag = 'begin'

# Main while loop
while counter < limit:
    print('Main loop iteration:', counter)

    # First level nested while loop
    inner_counter = 0
    while inner_counter < inner_limit:
        print('First level nested loop iteration:', inner_counter)

        if outer_flag == 'start':
            print('Outer flag is start')
            outer_flag = 'running'
            inner_sum = counter + inner_counter
        else:
            print('Outer flag is not start')
            outer_flag = 'start'
            inner_sum = counter - inner_counter

        # Second level nested while loop
        inner_inner_counter = 0
        while inner_inner_counter < 2:
            print('Second level nested loop iteration:', inner_inner_counter)

            if inner_flag == 'begin':
                print('Inner flag is begin')
                inner_flag = 'middle'
                inner_product = inner_sum * inner_inner_counter
            else:
                print('Inner flag is not begin')
                inner_flag = 'begin'
                inner_product = inner_sum / (inner_inner_counter + 1)

            inner_inner_counter = inner_inner_counter + 1

        inner_counter = inner_counter + 1

    counter = counter + 1

# End of the program
print('Program ended')
