counter = 0
threshold = 3
limit = 10
max_count = 5

while counter < 6:
    if counter == 0:
        if counter < limit:
            if threshold > 5:
                print('Zero, less than limit')
            else:
                print('Zero, limit OK')
    elif counter > 0:
        if counter % 2 == 0:
            if counter > threshold:
                if counter < limit:
                    print('Even, above threshold')
                elif counter == max_count:
                    print('Even, max count reached')
                else:
                    print('Even, above threshold, limit exceeded')
            else:
                print('Even, below threshold')
        else:
            if counter > threshold:
                print('Odd, above threshold')
            elif counter < limit:
                print('Odd, below limit')
            else:
                print('Odd, limit exceeded')
    if counter == 3:
        print('Counter is 3')

    counter = counter + 1

print('Done')
